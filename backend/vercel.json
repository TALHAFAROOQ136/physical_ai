{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "src/main.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
